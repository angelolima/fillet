<%- contentFor('CSScontainer') %>

<%- contentFor('JScontainer') %>

<%- contentFor('body') %>
<div class="jumbotron text-center">
    <h1>O Time!</h1>
    <div class="row">
      <table class="table table-dark">
        <thead>
          <tr>
            <th scope="col">#</th>
            <th scope="col">First</th>
            <th scope="col">Last</th>
            <th scope="col">Handle</th>
          </tr>
        </thead>
        <tbody>
          <% data.forEach(function (data) { %>
          <tr>
            <th scope="row"><%= data.codigo %></th>
            <td><%= data.transito %></td>
            <td><%= data.titulo %></td>
            <td><%= data.data_entrega %></td>
          </tr>
          <% }) %>
        </tbody>
      </table>
        <% for (usuario of usuarios){%>
            <div class="col-sm-4">
                <h2><%= usuario.name %></h2>
                <img class="img-responsive" style="height: 250px" src="<%= usuario.avatar %>">
            </div>
            <% } %>
    </div>
</div>
    